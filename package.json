{
    "name": "ie-dialog",
    "version": "0.0.5",
    "type": "module",
    "author": "June",
    "private": false,
    "description": "基于 konva.js 的简易编辑图片弹窗组件",
    "keywords": [
        "Konva",
        "Image-edit",
        "Canvas"
    ],
    "files": [
        "dist",
        "src",
        "index.d.ts"
    ],
    "main": "/dist/ie-dialog.umd.cjs",
    "module": "./dist/ie-dialog.es.js",
    "exports": {
        ".": {
            "require": "./dist/ie-dialog.umd.js",
            "import": "./dist/ie-dialog.es.js"
        },
        "./dist/style.css": "./dist/style.css"
    },
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc && vite build",
        "preview": "vite preview",
        "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
        "prettier": "prettier --write .",
        "prepare": "husky install",
        "commit": "git-cz"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-conventional-changelog"
        }
    },
    "dependencies": {
        "@types/lodash": "^4.14.191",
        "@types/lodash-es": "^4.17.6",
        "@vue/shared": "^3.2.47",
        "@vueuse/core": "^9.13.0",
        "clipboard": "^2.0.11",
        "csstype": "^2.6.20",
        "konva": "^8.4.2",
        "lodash-es": "^4.17.21",
        "lodash-unified": "^1.0.2",
        "pinia": "^2.0.29",
        "vue": "^3.2.45",
        "vue-router": "^4.1.6",
        "wx_signture": "^0.2.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.4.2",
        "@commitlint/config-conventional": "^17.4.2",
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "@vitejs/plugin-vue": "^4.0.0",
        "autoprefixer": "^10.4.13",
        "commitizen": "^4.2.6",
        "commitlint-config-cz": "^0.13.3",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.32.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.9.0",
        "husky": "^8.0.3",
        "postcss-plugin-px2rem": "^0.8.1",
        "prettier": "^2.8.3",
        "sass": "^1.57.1",
        "sass-loader": "^13.2.0",
        "terser": "^5.16.3",
        "typescript": "^4.9.3",
        "vite": "^4.0.0",
        "vue-tsc": "^1.0.11"
    },
    "homepage": "https://github.com/Qiu-Jun/dialog-edit-img#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/Qiu-Jun/dialog-edit-img.git"
    },
    "bugs": {
        "url": "https://github.com/Qiu-Jun/wx-signture/issues"
    }
}
